<?xml version="1.0" encoding="UTF-8"?>

<templates xml:space="preserve">

    <t t-name="awesome_clicker.client_action">
        <div class="ms-1 mt-1">
            <span>Clicks: <t t-esc="clickService.state.counter"/></span>
            <button class="btn btn-secondary ms-1" t-on-click="() => this.clickService.increment(9)">
                Increment
            </button>
        </div>
    </t>

</templates>
